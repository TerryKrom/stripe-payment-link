public with sharing class StripeService {

    private static final Stripe_Service_Setting__mdt settings = [
        SELECT Publishable_Key__c, Secret_Key__c, DeveloperName
        FROM Stripe_Service_Setting__mdt
        WHERE isActive__c = true
        LIMIT 1
    ];

    private static String publicKey = settings.Publishable_Key__c;
    private static String secretKey = settings.Secret_Key__c;

    public class Output {
        public Boolean success;
        public String message;
        public String data;
    }

    private static String convertJsonToFormUrlEncoded(String json) {
        if (json.contains('line_items')) {
            // Remover aspas e chaves do JSON
            // metadata:objectId:a1rPz00000V3h09IAB,line_items:quantity:1,price:price_1Qh6jxAc5MColC07b8BwDDJy

            json = json.replace('"', '')
                       .replace('{', '')
                       .replace('}', '')
                       .replace('[', '')
                       .replace(']', '');
    
            // Inicialize a nova string com a parte de line_items
            String newString = '';
            
            // Extrair os valores necessários
            String price = json.substringAfter('price:');
            // Criar a parte com o price_data, mantendo o formato correto com currency dentro de price_data
            newString += 'line_items[0][price]=' + price;  // Adicionar currency dentro de price_data
            newString += '&line_items[0][quantity]=1';  // Adicionar quantity, definido como 1
    
            System.debug(json);
            System.debug(newString);
            // Atualize o json com a nova string formatada

            String metadata = json.substringBetween('objectId:', ',');
            newString += '&[metadata][objectId]=' + metadata;

            json = newString;

        } else {
            String name;
            // Caso o JSON não tenha a chave 'line_items', tratar como um formato simples
            json = json.replace('{', '')
                       .replace('}', '')
                       .replace('"', '')
                       .replace(':', '=')
                       .replace(',', '&');
            if(json.contains('name')){
                // "name":"Do Not Delete - IT User Test - Italy"
                System.debug('JSON AQUI: ' + json);
                name = json.substringAfter('name=');
                json = json.substringBefore('name=');
                json += 'name=' + name;
                json = json.replace('=', ':');
                json = json.replaceFirst(':', '=');
                json = json.replace('name:', 'name=');
            }    
        }
    
        return json;
    }
    
    private static HttpRequest createHttpRequest(String endpoint, String method, String requestBody, Map<String, String> additionalHeaders) {
        HttpRequest request = new HttpRequest();
        request.setEndpoint(endpoint);
        request.setMethod(method);
        request.setHeader('Authorization', 'Bearer ' + secretKey);
        request.setHeader('Content-Type', 'application/x-www-form-urlencoded');
        
        // Se houver cabeçalhos adicionais, adiciona-os à requisição
        if (additionalHeaders != null) {
            for (String key : additionalHeaders.keySet()) {
                request.setHeader(key, additionalHeaders.get(key));
            }
        }
    
        // Se o corpo da requisição não for vazio, define-o
        if (!String.isBlank(requestBody)) {
            request.setBody(requestBody);
        }
        
        return request;
    }    

    // Helper to send requests
    private static HttpResponse sendRequest(HttpRequest request) {
        try {
            Http http = new Http();
            return http.send(request);
        } catch (Exception e) {
            throw new CalloutException('Error during HTTP callout: ' + e.getMessage());
        }
    }

    // Common method to handle responses
    private static Output handleResponse(HttpResponse response, Integer expectedStatusCode) {
        Output output = new Output();
        if (response.getStatusCode() == expectedStatusCode) {
            output.success = true;
            output.message = '';
        } else {
            output.success = false;
            output.message = 'Request failed with status code ' + response.getStatusCode() + ' and message: ' + response.getStatus();
        }
        output.data = response.getBody();
        return output;
    }

    public static Output createProduct(String requestBody) {
        // Converte o JSON para x-www-form-urlencoded
        String formUrlEncodedBody = convertJsonToFormUrlEncoded(requestBody);

        System.debug(requestBody);

        String endpoint = 'https://api.stripe.com/v1/products';
        System.debug('REQUISIÇÃO RECEBIDA PELA CLASSE SERVICE: ' + formUrlEncodedBody);

        HttpRequest request = createHttpRequest(endpoint, 'POST', formUrlEncodedBody, null);
        HttpResponse response = sendRequest(request);
        return handleResponse(response, 200);
    }

    public static Output createPrice(String requestBody) {
        String formUrlEncodedBody = convertJsonToFormUrlEncoded(requestBody);
        String endpoint = 'https://api.stripe.com/v1/prices';
        HttpRequest request = createHttpRequest(endpoint, 'POST', formUrlEncodedBody, null);
        HttpResponse response = sendRequest(request);
        return handleResponse(response, 200);
    }

    public static Output createPaymentLink(String requestBody) {
        // Converte o JSON para x-www-form-urlencoded
        String formUrlEncodedBody = convertJsonToFormUrlEncoded(requestBody);
        String endpoint = 'https://api.stripe.com/v1/payment_links';
        HttpRequest request = createHttpRequest(endpoint, 'POST', formUrlEncodedBody, null);
        HttpResponse response = sendRequest(request);
        return handleResponse(response, 200);
    }

