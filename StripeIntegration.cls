public with sharing class StripeIntegration {
    public String execute(Id recordId, Decimal amount, Integer depositPercentage, String paymentCurrency) {
        // Verificar o tipo de registro
        if (recordId.getSObjectType() == sugati__Supplier_Booking__c.SObjectType) {
            // Processo para Booking
            sugati__Supplier_Booking__c booking = getBookingById(recordId);
            if (booking == null) {
                return 'Booking not found';
            }
            return processStripeIntegrationForBooking(booking);
        } else if (recordId.getSObjectType() == Opportunity.SObjectType) {
            // Processo para Opportunity
            Opportunity opp = getOpportunityById(recordId);
            if (opp == null) {
                return 'Opportunity not found';
            }
            return processStripeIntegrationForOpportunity(opp, amount, depositPercentage, paymentCurrency);
        } else {
            return 'Invalid record type';
        }
    }
    
    private sugati__Supplier_Booking__c getBookingById(Id bookingId) {
        return [
            SELECT Id, Name, sugati__S_SB_Total_Gross_Cost_Booking_Currency__c,
                   sugati__SB_Itinerary_Description__c, sugati__S_SB_Service_Description__c,
                   sugati__SB_Supplier__r.sugati__S_Supplier_Currency__c, sugati__SB_Notes__c, 
                   sugati__SB_Supplier__r.Name, sugati__SB_From_Date__c
            FROM sugati__Supplier_Booking__c
            WHERE Id = :bookingId
            LIMIT 1
        ];
    }
    
    private Opportunity getOpportunityById(Id oppId) {
        return [
            SELECT Id,
            Name,
            sugati__S_O_Total_Gross_Sell_Price_Selling__c,
            Description,
            sugati__O_Booking_Currency__c,
            DepositPercentage__c
            FROM Opportunity
            WHERE Id = :oppId
            LIMIT 1
        ];
    }
    
    private String processStripeIntegrationForBooking(sugati__Supplier_Booking__c booking) {
        // Criar o produto no Stripe
        String productJson = createProductJsonForBooking(booking);
        StripeService.Output productResponse = StripeService.createProduct(productJson);
        if (!productResponse.success) {
            throw new AuraHandledException('Failed to create product: ' + productResponse.message);
        }
        
        // Criar o preço no Stripe
        Map<String, Object> productData = (Map<String, Object>) JSON.deserializeUntyped(productResponse.data);
        String productId = (String) productData.get('id');
        String priceJson = createPriceJsonForBooking(booking, productId);
        StripeService.Output priceResponse = StripeService.createPrice(priceJson);
        if (!priceResponse.success) {
            throw new AuraHandledException('Failed to create price: ' + priceResponse.message);
        }
        
        // Criar o link de pagamento
        return createPaymentLink(priceResponse, booking.Id);
    }
    
    private String processStripeIntegrationForOpportunity(Opportunity opp, Decimal amount, Integer depositPercentage, String paymentCurrency) {
        // Criar o produto no Stripe
        String productJson = createProductJsonForOpportunity(opp);
        StripeService.Output productResponse = StripeService.createProduct(productJson);
        if (!productResponse.success) {
            throw new AuraHandledException('Failed to create product: ' + productResponse.message);
        }
        
        // Criar o preço no Stripe
        Map<String, Object> productData = (Map<String, Object>) JSON.deserializeUntyped(productResponse.data);
        String productId = (String) productData.get('id');
        String priceJson = createPriceJsonForOpportunity(opp, productId, amount, depositPercentage, paymentCurrency);
        StripeService.Output priceResponse = StripeService.createPrice(priceJson);
        if (!priceResponse.success) {
            throw new AuraHandledException('Failed to create price: ' + priceResponse.message);
        }
        
        // Criar o link de pagamento
        return createPaymentLink(priceResponse, opp.Id);
    }
    
    private String createProductJsonForBooking(sugati__Supplier_Booking__c booking) {
        Map<String, Object> product = new Map<String, Object>();
        product.put('name', booking.Name);
        product.put('description', booking.sugati__SB_Itinerary_Description__c != null
            ? booking.sugati__SB_Itinerary_Description__c
            : booking.sugati__S_SB_Service_Description__c != null
            ? booking.sugati__S_SB_Service_Description__c
            : booking.sugati__SB_Notes__c != null
            ? booking.sugati__SB_Notes__c
            : 'Booking Reservation ' + booking.sugati__SB_From_Date__c.toString());
        return JSON.serialize(product);
    }
    
    private String createProductJsonForOpportunity(Opportunity opp) {
        Map<String, Object> product = new Map<String, Object>();
        product.put('name', opp.Name);
        product.put('description', opp.Description != null ? opp.Description : 'Opportunity Payment');
        return JSON.serialize(product);
    }
    
    private String createPriceJsonForBooking(sugati__Supplier_Booking__c booking, String productId) {
        Map<String, Object> price = new Map<String, Object>();
        price.put('unit_amount', Math.round(booking.sugati__S_SB_Total_Gross_Cost_Booking_Currency__c * 100));
        price.put('currency', booking.sugati__SB_Supplier__r.sugati__S_Supplier_Currency__c != null ? booking.sugati__SB_Supplier__r.sugati__S_Supplier_Currency__c : 'usd');
        price.put('product', productId);
        return JSON.serialize(price);
    }
    
    private String createPriceJsonForOpportunity(Opportunity opp, String productId, Decimal amount, Integer depositPercentage, String paymentCurrency) {
        Map<String, Object> price = new Map<String, Object>();
        
        // Definir a porcentagem (valor padrão 20%)
        Decimal percentage = depositPercentage != null
            ? depositPercentage
            : (opp.DepositPercentage__c != null
            ? opp.DepositPercentage__c
            : 20);

        if(amount != null && depositPercentage == null){
            percentage = 100;
        }        
        
        // Use amount como base, e sempre aplique a porcentagem
        Decimal baseAmount = amount != null 
            ? amount 
            : opp.sugati__S_O_Total_Gross_Sell_Price_Selling__c;

        Decimal oppAmount = baseAmount * (percentage / 100);
        
        // Definir a moeda (padrão 'usd' se nulo)
        String productCurrency = paymentCurrency != null
            ? paymentCurrency.toLowerCase()
            : (opp.sugati__O_Booking_Currency__c != null
            ? opp.sugati__O_Booking_Currency__c.toLowerCase()
            : 'usd');
        
        // Definir valor em centavos
        price.put('unit_amount', Math.round(oppAmount * 100));
        price.put('currency', productCurrency);
        price.put('product', productId);
        
        return JSON.serialize(price);
    }
    
    
    private String createPaymentLink(StripeService.Output priceResponse, String objectId) {
        Map<String, Object> priceData = (Map<String, Object>) JSON.deserializeUntyped(priceResponse.data);
        String priceId = (String) priceData.get('id');
        Map<String, Object> paymentLink = new Map<String, Object>();
        List<Object> lineItems = new List<Object>();
        Map<String, Object> lineItem = new Map<String, Object>();
        
        lineItem.put('price', priceId);
        lineItem.put('quantity', 1);
        lineItems.add(lineItem);
        
        paymentLink.put('line_items', lineItems);
        
        // Adiciona o metadata com o objectId
        Map<String, Object> metadata = new Map<String, Object>();
        metadata.put('objectId', objectId);
        paymentLink.put('metadata', metadata);
        
        // Cria o link de pagamento com o metadata
        StripeService.Output paymentLinkResponse = StripeService.createPaymentLink(JSON.serialize(paymentLink));
        if (!paymentLinkResponse.success) {
            throw new AuraHandledException('Failed to create payment link: ' + paymentLinkResponse.message);
        }
        
        Map<String, Object> paymentLinkData = (Map<String, Object>) JSON.deserializeUntyped(paymentLinkResponse.data);
        return (String) paymentLinkData.get('url');
    }
    
